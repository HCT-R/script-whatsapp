# Настройки WhatsApp уведомлений о грантах

# Задержка между сообщениями (в миллисекундах)
DELAY_BETWEEN_MESSAGES=5000

# Путь к Excel файлу с номерами телефонов
EXCEL_FILE_PATH=./phone_numbers.xlsx

# Настройки логирования
LOG_LEVEL=info
LOG_FILE=combined.log
ERROR_LOG_FILE=error.log

# Настройки Puppeteer (для WhatsApp Web)
PUPPETEER_HEADLESS=true
PUPPETEER_NO_SANDBOX=true 